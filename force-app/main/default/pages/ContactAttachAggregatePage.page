<apex:page controller="ContactAttachAgregateController">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockTable value="{!contactsModels}" var="contact">
                <apex:column headerValue="Contact Name" value="{!contact.contactName}" />
                <apex:column headerValue="Total Attachments" value="{!contact.attachmentCount}" />
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>